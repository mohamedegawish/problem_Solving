#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main() {
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll a,b,c;cin>>a>>b>>c;
    ll total=0;
    for(int i=1;i<=a;i++){
        ll sum=0;
        ll x=i;
        while(x>0){
 
               sum+= x%10;
 
            x=x/10;
 
        }
        if(sum<=c&&sum>=b){
            total+=i;
        }
    }
    cout<<total;
    return 0;
}
