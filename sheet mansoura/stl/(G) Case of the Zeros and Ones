#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll n,zeros=0,ones=0;cin>>n;
    string s;cin>>s;
    for(auto it:s){
        zeros+=(it=='0');
        ones+=(it=='1');
    }
    cout<<abs(zeros-ones);


    return 0;
}
