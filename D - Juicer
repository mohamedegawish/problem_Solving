
#include <bits/stdc++.h>
using namespace std;
#define ll long long
pair<int,int>a[1000];
int main() {
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll n,b,d;
    cin>>n>>b>>d;
    ll waste=0,countt=0;
    while(n--){
        ll orangeSize;
        cin>>orangeSize;
        if(orangeSize<=b){
            waste+=orangeSize;
        }
        if(waste>d){
            countt++;
            waste=0;
        }
    }
    
    cout<<countt;
}
