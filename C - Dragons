#include <bits/stdc++.h>
using namespace std;
#define ll long long
pair<int,int>a[1000];
int main() {
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll s,n;
    cin>>s>>n;
    ll CurrentPoints=s;
    bool flag=false;
    for(int i=0;i<n;i++){
        cin>>a[i].first>>a[i].second;

    }
    sort(a,a+n);
    for(int i=0;i<n;i++){
        if(a[i].first<CurrentPoints){
            CurrentPoints+=a[i].second;
            flag=true;
        }
        else{
            cout<<"NO";
            return 0;
        }
    }


    if(flag==true){
        cout<<"YES";
    }
    else{
        cout<<"NO";
    }
}
