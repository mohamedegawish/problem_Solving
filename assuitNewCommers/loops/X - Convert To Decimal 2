#include <bits/stdc++.h>
using namespace std;
#define ll long long
pair<int,int>a[1000];
int main() {
    ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
    ll cases;cin>>cases;
    while(cases--){
        ll num;cin>>num;
        ll onesCount=0;
        while(num>0){
            if (num%2==1) {
                onesCount++;
            }
            num=num/2;
        }
        ll binaryToDecimal=0,NextBinaryNum=1;
        for (int i=0;i<onesCount;i++) {
            if (i==0){

                binaryToDecimal+=1;
            }
            else {
                 NextBinaryNum=NextBinaryNum*2;
                binaryToDecimal+=NextBinaryNum;
            }
        }
        cout<<binaryToDecimal<<endl;

    }
}
